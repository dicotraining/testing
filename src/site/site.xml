<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
	name="${project.artifactId}">

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.0</version>
	</skin>
	<poweredBy>
		<logo name="" href="http://www.adamaconsulting.es" img="/img/logo1.gif" />
	</poweredBy>


	<custom>
		<fluidoSkin>
			<topBarEnabled>true</topBarEnabled>
			<topBarIcon>
				<name>Adama Consulting</name>
				<alt>Adama Consulting Homepage</alt>
				<src>/img/logo1.gif</src>
				<href>http://www.adamaconsulting.es</href>
			</topBarIcon>
			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
			<facebookLike />
			<googlePlusOne />
			<twitter>
				<user>adamaconsulting</user>
				<showUser>true</showUser>
				<showFollowers>true</showFollowers>
			</twitter>
		</fluidoSkin>
	</custom>

	<publishDate format="dd MM yyyy" />

	<version position="right" />

	<body>
		<menu name="Documentation">
		<item name="Home" href="/index.html"></item>
		</menu>
		<menu name="Modules" ref="modules" inherit="none" />
		<menu name="Reports" ref="reports" inherit="none" />

		<menu name="Links" inherit="none" inheritAsRef="true">
			<item name="Adama's Blog" href="http://adamaconsulting.blogspot.com.es/" />
			<item name="Github Homepage" href="http://www.github.com/dicotraining" />
			<item name="Linked In Profile" href="http://www.linkedin.com/in/davidpalomar" />
		</menu>
	</body>

</project>


















